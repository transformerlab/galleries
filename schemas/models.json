{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "uniqueID": {
        "type": "string"
      },
      "name": {
        "type": "string"
      },
      "description": {
        "type": "string"
      },
      "parameters": {
        "type": "string"
      },
      "context": {
        "type": "string"
      },
      "architecture": {
        "enum": [
          "LlamaForCausalLM",
          "T5ForConditionalGeneration",
          "FalconForCausalLM",
          "MistralForCausalLM",
          "MixtralForCausalLM",
          "GPTBigCodeForCausalLM",
          "PhiForCausalLM",
          "GGUF",
          "MLX",
          "GemmaForCausalLM",
          "CohereForCausalLM",
          "Qwen2ForCausalLM",
          "Phi3ForCausalLM"
        ]
      },
      "huggingface_repo": {
        "type": "string"
      },
      "huggingface_filename": {
        "type": "string"
      },
      "transformers_version": {
        "type": "string"
      },
      "license": {
        "type": "string"
      },
      "logo": {
        "type": "string"
      },
      "author": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "url": {
            "type": "string"
          },
          "blurb": {
            "type": "string"
          }
        },
        "required": ["name", "url", "blurb"]
      },
      "resources": {
        "type": "object",
        "properties": {
          "canonicalUrl": {
            "type": "string"
          },
          "downloadUrl": {
            "type": "string"
          },
          "paperUrl": {
            "type": "string"
          }
        },
        "required": ["canonicalUrl", "downloadUrl", "paperUrl"]
      }
    },
    "required": [
      "uniqueID",
      "name",
      "parameters",
      "architecture",
      "huggingface_repo",
      "license"
    ]
  }
}
